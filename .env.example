# Python Command Prefix
# command_prefix="apptainer exec --nv --writable-tmpfs --env NB_UID=${UID} --env NB_USER=${USER} --env XDG_CACHE_HOME=${HOME}/.cache/ csci4850-2022-09-10-compbio.sif"
